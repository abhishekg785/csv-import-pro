{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "entry",
      "loose": true,
      "targets": {
        "browsers": ["chrome >= 60", "safari >= 10"]
      }
    }],
    "@babel/preset-flow",
    ["@babel/preset-react", { "useBuiltIns": true }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    "@babel/syntax-object-rest-spread"
  ]
}
